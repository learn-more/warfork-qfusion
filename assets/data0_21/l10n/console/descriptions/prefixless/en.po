msgid "serverinfo"
msgstr "Displays server info settings in console.\n"
"^6Note: Values displayed: version, fs_game, g_antilag, g_gametype, g_instagib, protocol, sv_cheats, sv_hostname, sv_http, sv_maxclients, sv_maxmvclients, sv_mm_enable, sv_mm_loginonly, sv_pps, sv_pure, sv_skilllevel, sv_skillRating."

#. Console/prefixless
msgid "serverrecord"
msgstr "<name> Records a multiPOV demo on the Server.\n"
"^6Note: Can be downloaded by clients via demolist and demoget."  

#. Console/prefixless
msgid "serverrecordcancel"
msgstr "Terminates the recording of the server demo and deletes the recorded file."

#. Console/prefixless
msgid "serverrecordpurge"
msgstr "Purges the recorded server demo files."

#. Console/prefixless
msgid "serverrecordstop"
msgstr "Stops server demo recording."

#. Console/prefixless
msgid "set"
msgstr "<cvar> <value> Changes the value of x cvar to y."

