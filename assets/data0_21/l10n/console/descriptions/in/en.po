#. Console/in
msgid "in_debug"
msgstr "<0/1> Debugs keyboard input.\n"
"^6Disable: in_debug \"0\"\n"
"^6Enable: in_debug \"1\"\n"
"^6Example: key:100 down:0 time:1405849 +moveright"

#. Console/in
msgid "in_dinput"
msgstr "<0/1> DirectInput.\n"
"^6Disable: in_dinput \"0\"\n"
"^6Enable: in_dinput \"1\""

#. Console/in
msgid "in_grabinconsole"
msgstr "Mouse cursor will be locked to the game (in window mode) when the console is open.\n"
"^6Disable: in_grabinconsole \"0\"\n"
"^6Enable: in_grabinconsole \"1\""

#. Console/in
msgid "in_initmouse"
msgstr "Initializes the mouse on game launch.\n"
"^6Note: This command was intended for development purposes and only works using Microsoft Windows. If you want to disable mouse input use in_mouse instead."

#. Console/in
msgid "in_mouse"
msgstr "<0/1> Mouse input.\n"
"^6Disable: in_mouse \"0\"\n"
"^6Enable: in_mouse \"1\"\n"
"^6Note: This command only works using Microsoft Windows."
