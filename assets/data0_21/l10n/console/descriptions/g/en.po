msgid "g_spawnsystem_wave_time"
msgstr "<value> Maximum duration of time a spawn sound can play."

#. Console/g
msgid "g_teams_allow_uneven"
msgstr "<0/1> Allow the teams to be uneven.\n"
"^6Disable: g_teams_allow_uneven \"0\"\n"
"^6Enable: g_teams_allow_uneven \"1\"\n"
"^6Note: If enabled, allows players to switch teams anytime they want."

#. Console/g
msgid "g_teams_maxplayers"
msgstr "<value> How many players a single team can have."

#. Console/g
msgid "g_timelimit"
msgstr "<value> Sets the duration of matches (in minutes)."

#. Console/g
msgid "g_uploads_demos"
msgstr "<0/1> Allows players to download demos from the server.\n"
"^6Disable: g_uploads_demos \"0\"\n"
"^6Enable: g_uploads_demos \"1\""

#. Console/g
msgid "g_votable_gametypes"
msgstr "<value> A list of gametypes allowed to be used in \"gametype\" callvote."

#. Console/g
msgid "g_vote_allowed"
msgstr "<0/1> Vote calling (including opcalls).\n"
"^6Disable: g_vote_allowed \"0\"\n"
"^6Enable: g_vote_allowed \"1\""

#. Console/g
msgid "g_vote_cooldowntime"
msgstr "<value> The time a player needs to wait (in seconds) after calling a vote to call another one."

#. Console/g
msgid "g_vote_electtime"
msgstr "<value> Specifies how long callvotes last (in seconds)."

#. Console/g
msgid "g_vote_maxchanges"
msgstr "<value> Specifies how many times can a player change their vote."

#. Console/g
msgid "g_vote_percent"
msgstr "<0-100> The percentage of players that need to vote yes for it to pass."

#. Console/g
msgid "g_weapon_respawn"
msgstr "<value> The amount of time (in seconds) weapons are respawned.\n"
"^6Note: If you set g_weapon_respawn = \"0\", it'll use the gametypes defined value instead."
